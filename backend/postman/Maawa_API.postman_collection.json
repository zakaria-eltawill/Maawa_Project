								"key": "Authorization",
								"value": "Bearer {{access_token}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "X-Idempotency-Key",
								"value": "{{$randomUUID}}",
								"description": "Idempotency key (required)"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"property_id\": \"{{property_id}}\",\n    \"check_in\": \"2025-12-01\",\n    \"check_out\": \"2025-12-05\",\n    \"guests\": 2\n}",

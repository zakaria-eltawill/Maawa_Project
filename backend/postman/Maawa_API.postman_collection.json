							"raw": "{\n    \"name\": \"Osama Ellafi\",\n    \"email\": \"osama@example.com\",\n    \"password\": \"osama123\",\n    \"password_confirmation\": \"osama123\",\n    \"role\": \"owner\",\n    \"phone_number\": \"+218912345678\",\n    \"region\": \"Benghazi\"\n}",
